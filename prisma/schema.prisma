datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model Bus{
    id Int @id @default(autoincrement())
    origin String
    destination String
    seats Int
    bookedSeats bookedSeats[]
}



model bookedSeats{
    id Int @id @default(autoincrement())
    number Int
    Bus Bus? @relation(fields: [busId], references: [id])
    busId Int?
    @@index([busId])
}
